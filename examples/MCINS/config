[MESH]
filename = unit_square_coarse.vol

[DIM]
diffuse_interface_method = False

[FINITE ELEMENT SPACE]
elements = u -> VectorH1
           p -> H1
           a -> H1
           b -> H1
interpolant_order = 3

[DG]
DG = False

[SOLVER]
solver = default
preconditioner = default
linearization_method = IMEX

[TRANSIENT]
transient = True
scheme = euler IMEX
time_range = 0.0, 10
dt = 0.003

[VISUALIZATION]
save_to_file = True
save_type = .vtu
save_frequency = 1, numit
subdivision = 3

[ERROR ANALYSIS]
check_error = True

[OTHER]
num_threads = 6
model = MultiComponentINS
component_names = a, b
component_in_error_calc = a -> True
                          b -> True
component_in_time_deriv = a -> True
                          b -> True
run_dir = .
