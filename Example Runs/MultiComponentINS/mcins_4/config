[MESH]
filename = mesh_files/unit_square_coarse.vol

[DIM]
diffuse_interface_method = False

[FINITE ELEMENT SPACE]
elements = u -> VectorH1
           p -> H1
           a -> H1
           b -> H1
interpolant_order = 3

[DG]
DG = False

[SOLVER]
solver = default
preconditioner = default
linearization_method = IMEX
nonlinear_tolerance = relative -> 1e-4
                      absolute -> 1e-8
nonlinear_max_iterations = 10

[TRANSIENT]
transient = True
scheme = SBDF
time_range = 0.0, 10
dt = 0.003
dt_range = 1e-16, 5
dt_tolerance = relative -> 1e-6
               absolute -> 5e-4

[VISUALIZATION]
save_to_file = True
save_type = .vtu
save_frequency = 1, numit
subdivision = 3

[ERROR ANALYSIS]
check_error = True

[OTHER]
num_threads = 6
model = MultiComponentINS
component_names = a, b
component_in_error_calc = a -> True
                          b -> True
component_in_time_deriv = a -> True
                          b -> True
run_dir = Example Runs/MultiComponentINS/mcins_4
