[REFERENCE SOLUTIONS]
u = [0.0, 0.0]
# k = 5
a =     exp(-5*(1-3/sqrt(10))*t) * (5 - 5*(1+3/sqrt(10)))/(-3*sqrt(10)) * 1 - exp(-5*(1+3/sqrt(10))*t) * (5 - 5*(1-3/sqrt(10)))/(-3*sqrt(10)) * 1
b =     exp(-5*(1-3/sqrt(10))*t) * (5 - 5*(1+3/sqrt(10)))/(-3*sqrt(10)) * (5 - 5*(1-3/sqrt(10))) / (0.9*5) - exp(-5*(1+3/sqrt(10))*t) * (5 - 5*(1-3/sqrt(10)))/(-3*sqrt(10)) * (5 - 5*(1+3/sqrt(10))) / (0.9*5)
c = 1 - exp(-5*(1-3/sqrt(10))*t) * (5 - 5*(1+3/sqrt(10)))/(-3*sqrt(10)) * (1.9*5 - 5*(1-3/sqrt(10))) / (0.9*5) + exp(-5*(1+3/sqrt(10))*t) * (5 - 5*(1-3/sqrt(10)))/(-3*sqrt(10)) * (1.9*5 - 5*(1+3/sqrt(10))) / (0.9*5)

[METRICS]
L2_norm = a, b, c
