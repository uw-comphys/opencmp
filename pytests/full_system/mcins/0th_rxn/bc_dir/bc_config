[DIRICHLET]
u = top    -> [0.0, 0.0]
    bottom -> [0.0, 0.0]
    left   -> [0.0, 0.0]
# NOTE: using exp instead of step change to avoid strange oscillation behaviour.
;a = right -> exp(-t*10)
;b = left  -> 1 - exp(-t*10)

[NEUMANN]
a = top    -> 0
    bottom -> 0
    left   -> 0
    right  -> 0
b = top    -> 0
    bottom -> 0
    left   -> 0
    right  -> 0

[STRESS]
stress = right -> [0.0, 0.0]
